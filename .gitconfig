[user]
    email = castlest@gmail.com
	  name = Sam Castle
[credential]
	  helper = cache --timeout=3600
[push]
    default = simple
[alias]
    hist = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
    ss = status -sb
    head = "!git --no-pager log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --max-count=10"
[core]
    editor = vim
[color]
    branch = auto
    diff = auto
    status = auto
